<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gokul.todoapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_playlist_add_check_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_playlist_add_check_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.gokul.todoapplication.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>